unit UOwner;

interface

uses
  myLib;

type
  TOwner = class
  protected
    name: string;
    Grounding: RGroundingContext;
  public
    function GetName: string;
    function GetGroundingContext: RGroundingContext;
    procedure SetGroundingContext(context: RGroundingContext);
  end;
  
implementation

function TOwner.GetName: string;
begin
  result := name;
end;

function TOwner.GetGroundingContext: RGroundingContext;
begin
  result := Grounding;
end;

procedure TOwner.SetGroundingContext(context: RGroundingContext);
begin
  Grounding := context;
end;

end.
 